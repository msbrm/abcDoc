{
  "name": "abc-doc",
  "displayName": "abcDoc",
  "description": "Convenient to add code documentation.",
  "version": "0.0.1",
  "publisher": "msbrm",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Language Settings",
      "properties": {
        "abcDoc.languageSettings.python": {
          "type": "boolean",
          "default": true,
          "description": "Enable for Python"
        },
        "abcDoc.languageSettings.java": {
          "type": "boolean",
          "default": false,
          "description": "Enable for Java"
        },
        "abcDoc.languageSettings.cpp": {
          "type": "boolean",
          "default": false,
          "description": "Enable for C++"
        }
      }
    },
    "languages": [
      {
        "id": "python",
        "extensions": [
          ".py"
        ],
        "aliases": [
          "Python",
          "python"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "cpp",
        "extensions": [
          ".cpp",
          ".h",
          ".hpp"
        ],
        "aliases": [
          "C++",
          "cpp"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "java",
        "extensions": [
          ".java"
        ],
        "aliases": [
          "Java",
          "java"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "completionItems": [],
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-python": "^0.21.0",
    "vscode-languageclient": "^9.0.1"
  }
}
